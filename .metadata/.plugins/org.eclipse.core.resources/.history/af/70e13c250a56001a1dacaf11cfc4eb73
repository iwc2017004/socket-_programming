package servers;

import java.net.*;
public class MyServer {

	public static void main(String[] args) {
		try {
			ServerSocket sk = new ServerSocket(80);
			System.out.println("Server is started");
			Socket s = sk.accept();
			System.out.println(s);
			sk.close();
			
		}catch(Exception e) {
			System.out.println(e);
			}
		

	}

}
